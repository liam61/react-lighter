{
  "name": "react-lighter",
  "version": "2.0.0",
  "description": "initialize your react project",
  "basePath": "./src",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.config.js --progress",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.config.js --progress",
    "dll": "webpack --config config/webpack.dll.config.js --progress",
    "serve": "hs dist"
  },
  "author": "lawler61",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lawler61/react-lighter.git"
  },
  "homepage": "https://github.com/lawler61/react-lighter",
  "keywords": [
    "h5",
    "react",
    "mobx",
    "axios",
    "antd"
  ],
  "browserslist": [
    "> 1%",
    "iOS >= 8",
    "Android >= 4.4",
    "not ie < 11",
    "Firefox ESR"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write src/**/*.{ts,tsx}"
    ],
    "src/**/*.js": [
      "prettier --write src/**/*.js"
    ],
    "src/**/*.{less,css}": [
      "stylelint --syntax less --fix",
      "prettier --write src/**/*.{less,css}"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "prettier --write src/**/*.scss"
    ],
    "config/**/*.js": [
      "prettier --write config/**/*.js"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs2": "7.11.2",
    "antd-mobile": "^2.3.4",
    "axios": "1.6.0",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.0",
    "qs": "6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.2",
    "@types/qs": "6.9.4",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/parser": "3.9.1",
    "add-asset-html-webpack-plugin": "3.1.3",
    "autoprefixer": "9.8.6",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.0",
    "chalk": "4.1.0",
    "clean-webpack-plugin": "1.0.0",
    "commander": "6.0.0",
    "cross-env": "7.0.2",
    "css-loader": "4.2.1",
    "cssnano": "4.1.10",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "6.0.0",
    "fs-extra": "9.0.1",
    "glob-all": "3.2.1",
    "happypack": "5.0.1",
    "html-webpack-plugin": "4.3.0",
    "http-server": "0.12.3",
    "husky": "4.2.5",
    "invariant": "2.2.4",
    "less": "3.12.2",
    "less-loader": "6.2.0",
    "lint-staged": "10.2.11",
    "mini-css-extract-plugin": "0.10.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-aspect-ratio-mini": "1.1.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.3",
    "postcss-preset-env": "6.7.0",
    "postcss-px-to-viewport": "1.1.1",
    "postcss-url": "8.0.0",
    "postcss-viewport-units": "0.1.6",
    "postcss-write-svg": "3.0.1",
    "prettier": "2.0.5",
    "purgecss-webpack-plugin": "2.3.0",
    "react-refresh": "^0.8.3",
    "sass-loader": "9.0.3",
    "style-loader": "1.2.1",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "ts-loader": "8.0.3",
    "typescript": "3.9.7",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.1.1",
    "webpack-parallel-uglify-plugin": "1.1.2"
  }
}
